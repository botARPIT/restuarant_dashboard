# Cloudflare Configuration
CLOUDFLARE_ACCOUNT_ID=your_account_id
CLOUDFLARE_API_TOKEN=your_api_token

# Environment
ENVIRONMENT=development
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# Platform API Configuration
SWIGGY_API_URL=https://api.swiggy.com
SWIGGY_PARTNER_ID=your_swiggy_partner_id
SWIGGY_API_KEY=your_swiggy_api_key
SWIGGY_API_SECRET=your_swiggy_api_secret

ZOMATO_API_URL=https://api.zomato.com
ZOMATO_PARTNER_ID=your_zomato_partner_id
ZOMATO_API_KEY=your_zomato_api_key
ZOMATO_API_SECRET=your_zomato_api_secret

UBEREATS_API_URL=https://api.ubereats.com
UBEREATS_PARTNER_ID=your_ubereats_partner_id
UBEREATS_API_KEY=your_ubereats_api_key
UBEREATS_API_SECRET=your_ubereats_api_secret

DOORDASH_API_URL=https://api.doordash.com
DOORDASH_PARTNER_ID=your_doordash_partner_id
DOORDASH_API_KEY=your_doordash_api_key
DOORDASH_API_SECRET=your_doordash_api_secret

# Database Configuration
DATABASE_URL=your_database_connection_string
REDIS_URL=your_redis_connection_string

# External Services
UPSTASH_REDIS_REST_URL=your_upstash_redis_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token

PLANETSCALE_HOST=your_planetscale_host
PLANETSCALE_USERNAME=your_planetscale_username
PLANETSCALE_PASSWORD=your_planetscale_password

# Encryption & Security
ENCRYPTION_KEY=your-32-character-encryption-key
SESSION_SECRET=your-session-secret-key

# Email & Notifications
SENDGRID_API_KEY=your_sendgrid_api_key
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Analytics & Monitoring
SENTRY_DSN=your_sentry_dsn
MIXPANEL_TOKEN=your_mixpanel_token
GOOGLE_ANALYTICS_ID=your_google_analytics_id

# Payments
STRIPE_PUBLIC_KEY=your_stripe_public_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

RAZORPAY_KEY_ID=your_razorpay_key_id
RAZORPAY_KEY_SECRET=your_razorpay_key_secret

# Feature Flags
ENABLE_BETA_FEATURES=false
ENABLE_ANALYTICS=true
ENABLE_NOTIFICATIONS=true
ENABLE_REAL_TIME_SYNC=true

# Rate Limiting
SWIGGY_RATE_LIMIT=100
ZOMATO_RATE_LIMIT=1000
UBEREATS_RATE_LIMIT=500
DOORDASH_RATE_LIMIT=50

# Caching
CACHE_TTL_ORDERS=300
CACHE_TTL_MENU=3600
CACHE_TTL_ANALYTICS=1800
CACHE_TTL_USER_SESSION=86400

# Webhook URLs (for platform integrations)
WEBHOOK_BASE_URL=https://your-domain.com/api/v1/webhooks
SWIGGY_WEBHOOK_SECRET=your_swiggy_webhook_secret
ZOMATO_WEBHOOK_SECRET=your_zomato_webhook_secret
UBEREATS_WEBHOOK_SECRET=your_ubereats_webhook_secret
DOORDASH_WEBHOOK_SECRET=your_doordash_webhook_secret